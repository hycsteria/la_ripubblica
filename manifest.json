{
  "manifest_version": 2,

  "name": "La RIpubblica",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "2.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}

